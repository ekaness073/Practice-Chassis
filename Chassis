package org.tahomarobotics.robot.chassis;


public class Chassis {

    private final static CANSparkMax LEFT_FRONT_MOTOR(RobotMap.LEFT_FRONT_MOTOR, CANSparkMax.MotorType.kbrushless);
    private final static CANSparkMax LEFT_BACK_MOTOR(RobotMap.LEFT_BACK_MOTOR, CANSparkMax.MotorType.kbrushless);
    private final static CANSparkMax Right_FRONT_MOTOR(RobotMap.RIGHT_FRONT_MOTOR, CANSparkMax.MotorType.kbrushless);
    private final static CANSparkMax RIGHT_BACK_MOTOR(RobotMap.RIGHT_FRONT_MOTOR, CANSparkMax.MotorType.kbrushless);
    public static(main){
        LEFT_FRONT_MOTOR.invert(true);
        LEFT_BACK_MOTOR.invert(true);

        public void setPower (Double ForwardPower Double rotationalPower){
            LEFT_FRONT_MOTOR().set(left);
            Right_FRONT_MOTOR(). set(right);
        }
        public void setIdleMode(CANSparkMax.IdleMode mode){
            LEFT_FRONT_MOTOR.setIdleMode(mode);
            LEFT_BACK_MOTOR.setIdleMode(mode);
            RIGHT_FRONT_MOTOR.setIdleMode(mode);
            RIGHT_BACK_MOTOR.setIdleMode(mode);

        } 

    }


}
